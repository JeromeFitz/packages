{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@jeromefitz/src/tsconfig-node-16",
  "extends": "./base.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES2022"],
    "module": "Node16",
    "moduleResolution": "Node16",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES2022"
  }
}
