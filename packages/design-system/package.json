{
  "name": "@jeromefitz/design-system",
  "version": "0.0.0",
  "description": "Design System for JeromeFitzgerald.com (Radix UI)",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "keywords": [
    "design system",
    "radix-ui"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/jeromefitz/packages.git",
    "directory": "packages/design-system"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "scripts": {
    "build": "tsup-node",
    "dev": "tsup-node --watch",
    "---": "",
    "clean": "rm -rf .turbo && rm -rf dist",
    "clean:install": "yarn clean && rm -rf node_modules",
    "copy": "mkdir -p ./dist && ./scripts/copy.sh",
    "--- ": "",
    "lint:eslint": "eslint ./src --ext cjs,js,jsx,mjs,ts,tsx --max-warnings=0",
    "lint:prettier": "prettier \"./src/**/*.{cjs,js,jsx,mjs,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\" --ignore-unknown --loglevel warn",
    "lint": "yarn lint:prettier --check && yarn lint:eslint",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:prettier --write",
    "---  ": "",
    "semantic-release": "semantic-release",
    "semantic-release:mono": "semantic-release -e semantic-release-monorepo"
  },
  "dependencies": {
    "@mantine/hooks": "4.1.4",
    "@radix-ui/colors": "0.1.8",
    "@radix-ui/react-accessible-icon": "0.1.4",
    "@radix-ui/react-accordion": "0.1.6",
    "@radix-ui/react-alert-dialog": "0.1.7",
    "@radix-ui/react-announce": "0.1.4",
    "@radix-ui/react-aspect-ratio": "0.1.4",
    "@radix-ui/react-avatar": "0.1.4",
    "@radix-ui/react-checkbox": "0.1.5",
    "@radix-ui/react-compose-refs": "0.1.0",
    "@radix-ui/react-context-menu": "0.1.6",
    "@radix-ui/react-dialog": "0.1.7",
    "@radix-ui/react-dropdown-menu": "0.1.6",
    "@radix-ui/react-icons": "1.1.0",
    "@radix-ui/react-navigation-menu": "0.1.2",
    "@radix-ui/react-popover": "0.1.6",
    "@radix-ui/react-progress": "0.1.4",
    "@radix-ui/react-radio-group": "0.1.5",
    "@radix-ui/react-scroll-area": "0.1.4",
    "@radix-ui/react-select": "0.1.1",
    "@radix-ui/react-separator": "0.1.4",
    "@radix-ui/react-slider": "0.1.4",
    "@radix-ui/react-switch": "0.1.5",
    "@radix-ui/react-tabs": "0.1.5",
    "@radix-ui/react-toast": "0.1.1",
    "@radix-ui/react-toggle": "0.1.4",
    "@radix-ui/react-toggle-group": "0.1.5",
    "@radix-ui/react-toolbar": "0.1.5",
    "@radix-ui/react-tooltip": "0.1.7",
    "@radix-ui/react-utils": "0.0.5",
    "@radix-ui/react-visually-hidden": "0.1.4",
    "@react-aria/interactions": "3.8.3",
    "@stitches/react": "1.2.7",
    "emoji-regex": "10.1.0",
    "framer-motion": "6.3.0",
    "kbar": "0.1.0-beta.33",
    "lodash": "4.17.21",
    "node-emoji": "1.11.0",
    "react-use": "17.3.2",
    "smoothscroll-polyfill": "0.4.4",
    "swr": "1.3.0"
  },
  "devDependencies": {
    "@types/lodash": "4.14.181",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "next": "12.1.5-canary.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "typescript": "4.5.5"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "swr": "^1.2.2"
  },
  "type": "module",
  "sideEffects": false,
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js"
}
