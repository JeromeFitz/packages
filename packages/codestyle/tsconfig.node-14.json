{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@jeromefitz/codestyle/node-14",
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "strict": true,
    "target": "es2020"
  }
}
