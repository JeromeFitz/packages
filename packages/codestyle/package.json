{
  "name": "@jeromefitz/codestyle",
  "version": "0.0.0",
  "description": "ESLint + TypeScript + Prettier + Lint Staged",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/jeromefitz/packages.git",
    "directory": "packages/codestyle"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "scripts": {
    "build": "tsc",
    "lint-eslint": "eslint . --ext cjs,js,jsx,mjs,ts,tsx --max-warnings=0",
    "lint-fix": "yarn prettier-fix && eslint . --ext cjs,js,jsx,mjs,ts,tsx --fix --max-warnings=0",
    "lint-typescript": "yarn typescript",
    "lint": "yarn lint-typescript && yarn prettier-check && yarn lint-eslint",
    "tsc:watch": "tsc -w --preserveWatchOutput",
    "prettier-check": "prettier --check \"**/*.{cjs,js,jsx,mjs,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\"",
    "prettier-fix": "prettier --write \"**/*.{cjs,js,jsx,mjs,ts,tsx,json,md,mdx,css,html,yml,yaml,scss}\"",
    "pretty-quick": "pretty-quick",
    "semantic-release": "echo semantic-release",
    "test": "echo skip @jeromefitz/codestyle test",
    "typescript": "tsc --noEmit --declaration"
  },
  "dependencies": {
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.3",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "eslint": "7.32.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jest": "25.3.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "lint-staged": "12.1.2",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.2",
    "typescript": "4.5.2"
  },
  "type": "module",
  "bin": {
    "jeromefitz-codestyle": "dist/index.js",
    "jeromefitz-pretty-quick": "./node_modules/pretty-quick/bin/pretty-quick.js"
  },
  "files": [
    ".eslintrc.cjs",
    ".eslintrc.next.cjs",
    ".eslintrc.react.cjs",
    ".eslintrc.react-native.cjs",
    ".prettierrc.cjs",
    ".prettierrc.react.cjs",
    ".prettierrc.react-native.cjs",
    "dist/index.js",
    "index.js",
    "lint-staged.config.js",
    "tsconfig.jest.json",
    "tsconfig.node.json",
    "tsconfig.react-native.json",
    "tsconfig.react.json"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./.eslintrc.cjs": "./.eslintrc.cjs",
    "./.eslintrc.next.cjs": "./.eslintrc.next.cjs",
    "./.eslintrc.react-native.cjs": "./.eslintrc.react-native.cjs",
    "./.eslintrc.react.cjs": "./.eslintrc.react.cjs",
    "./.prettierrc.cjs": "./.prettierrc.cjs",
    "./.prettierrc.react-native.cjs": "./.prettierrc.react-native.cjs",
    "./.prettierrc.react.cjs": "./.prettierrc.react.cjs",
    "./lint-staged.config.js": "./lint-staged.config.js",
    "./tsconfig.jest.json": "./tsconfig.jest.json",
    "./tsconfig.node.json": "./tsconfig.node.json",
    "./tsconfig.react-native.json": "./tsconfig.react-native.json",
    "./tsconfig.react.json": "./tsconfig.react.json"
  }
}
