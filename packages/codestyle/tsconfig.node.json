{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "@jeromefitz/codestyle/node",
  "compilerOptions": {
    "lib": ["ES2020"],
    "module": "ES2020",
    "target": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true
  }
}
