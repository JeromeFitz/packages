{
  "name": "@jeromefitz/semantic",
  "version": "0.0.0",
  "description": "Semantic Versioning, Conventional Commits with CI/CD for GitHub Actions.",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/jeromefitz/packages.git",
    "directory": "packages/semantic"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "scripts": {
    "build": "yarn clean && tsup src/* --format esm,cjs --dts --minify --no-splitting --target node16",
    "dev": "tsup src/* --format esm,cjs --dts --watch --minify --no-splitting --target node16",
    "clean": "rm -rf .turbo && rm -rf dist",
    "clean:install": "yarn clean && rm -rf node_modules"
  },
  "devDependencies": {
    "@types/lodash": "4.14.178"
  },
  "dependencies": {
    "@jeromefitz/semantic-release-git": "10.0.0-develop.2",
    "@octokit/core": "3.5.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.2",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "grapheme-splitter": "1.0.4",
    "lodash": "4.17.21",
    "multi-semantic-release": "2.11.0",
    "semantic-release": "18.0.1",
    "semantic-release-commit-filter": "1.0.2",
    "title": "3.4.3"
  },
  "type": "module",
  "files": [
    "dist/**",
    "package.json",
    "README.md"
  ],
  "exports": {
    "./changelog": "./dist/changelog.config.js",
    "./release.config.cjs": "./dist/release.config.cjs"
  }
}
