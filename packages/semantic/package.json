{
  "name": "@jeromefitz/semantic",
  "version": "0.0.0",
  "description": "Semantic Versioning, Conventional Commits with CI/CD for GitHub Actions.",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/jeromefitz/packages.git",
    "directory": "packages/semantic"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "scripts": {
    "build": "tsup-node src/*",
    "dev": "tsup-node src/* --watch",
    "---": "",
    "clean": "rm -rf .turbo && rm -rf dist",
    "clean:install": "yarn clean && rm -rf node_modules",
    "copy": "mkdir -p ./dist && ./scripts/copy.sh",
    "----": "",
    "semantic-release": "yarn copy && semantic-release",
    "semantic-release:mono": "yarn copy && semantic-release -e semantic-release-monorepo"
  },
  "devDependencies": {
    "@types/lodash": "4.14.178",
    "@types/title": "3.4.1"
  },
  "dependencies": {
    "@jeromefitz/conventional-gitmoji": "*",
    "@octokit/core": "3.5.1",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.2",
    "@semantic-release/npm": "8.0.3",
    "@semantic-release/release-notes-generator": "10.0.3",
    "grapheme-splitter": "1.0.4",
    "lodash": "4.17.21",
    "semantic-release": "19.0.2",
    "semantic-release-commit-filter": "1.0.2",
    "title": "3.4.3"
  },
  "type": "module",
  "sideEffects": false,
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js"
}
