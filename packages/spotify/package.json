{
  "name": "@jeromefitz/spotify",
  "version": "0.0.0",
  "description": "Spotify Wrapper for: Now Playing, Top Artists, & Top Tracks",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "keywords": [
    "spotify"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/jeromefitz/packages.git",
    "directory": "packages/spotify"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "scripts": {
    "build": "yarn clean && tsup src/* --format esm,cjs --dts --minify --no-splitting --target node16",
    "dev": "tsup src/* --format esm,cjs --watch --dts --minify --no-splitting --target node16",
    "clean": "rm -rf .turbo && rm -rf dist",
    "clean:install": "yarn clean && rm -rf node_modules",
    "semantic-release": "semantic-release -e semantic-release-monorepo"
  },
  "dependencies": {
    "isomorphic-unfetch": "3.1.0",
    "github-slugger": "1.4.0",
    "plaiceholder": "2.2.0"
  },
  "type": "module",
  "files": [
    "dist/**",
    "package.json",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts"
}
