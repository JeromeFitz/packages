{
  "name": "@jeromefitz/notion",
  "version": "0.0.0",
  "description": "Notion: Custom Wrapper",
  "author": {
    "name": "Jerome Fitzgerald",
    "email": "j@jeromefitzgerald.com",
    "url": "https://jeromefitzgerald.com"
  },
  "keywords": [
    "notion"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/jeromefitz/packages.git",
    "directory": "packages/notion"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "private": false,
  "scripts": {
    "build": "yarn clean && mkdir -p ./dist && yarn copy && tsup-node src/*",
    "dev": "yarn clean && mkdir -p ./dist && yarn copy && tsup-node src/* --watch",
    "clean": "rm -rf .turbo && rm -rf dist",
    "clean:install": "yarn clean && rm -rf node_modules",
    "copy": "./scripts/copy.sh",
    "semantic-release": "yarn copy && semantic-release -e semantic-release-monorepo"
  },
  "dependencies": {
    "@notionhq/client": "0.4.12",
    "date-fns": "2.28.0",
    "date-fns-tz": "1.2.2",
    "isomorphic-unfetch": "3.1.0",
    "github-slugger": "1.4.0",
    "lodash": "4.17.21",
    "plaiceholder": "2.2.0"
  },
  "devDependencies": {
    "@types/github-slugger": "1.3.0",
    "@types/lodash": "4.14.178"
  },
  "type": "module",
  "sideEffects": false,
  "typings": "./index.d.ts",
  "main": "./index.cjs",
  "module": "./index.js",
  "files": [
    "!release.config.cjs"
  ]
}
